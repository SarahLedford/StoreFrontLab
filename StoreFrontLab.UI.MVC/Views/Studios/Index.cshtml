@model IEnumerable<StoreFrontLab.DATA.EF.Studio>

@{
    ViewBag.Title = "Movie Studios";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Add a New Studio", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudioName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudioName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StudioID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StudioID })
        </td>
    </tr>
}

</table>
